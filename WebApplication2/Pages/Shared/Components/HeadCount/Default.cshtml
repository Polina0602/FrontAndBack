@model IEnumerable<RoleHeadCount>

    <table class="table table-hover rounded-pill">
        <thead class="thead-light">
            <tr>
                <th>
                    Role
                </th>
                <th>
                    Players
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model){
                <tr>
                    <td>@item.Role</td>
                    <td>@item.Count</td>
                </tr>
            }
        </tbody>
    </table>
